<div class="content-wrapper">
    <section class="content">
        <form id="formEmpresa">
            <div class="container-fluid">
                <div class="card card-default">
                    <div class="card-header ">
                        <h3 class="card-title">Cadastro De Empresa</h3>
                        <div class="card-tools">
                            <button type="button" class="btn btn-tool" data-card-widget="collapse"><i
                                    class="fas fa-minus"></i></button>
                        </div>
                    </div>
                    <div class="card-body">
                        <div class="row" style="margin-top: 20px;">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label for="razaoSicial">Razão Social:</label>
                                    <input type="text" data-val="true" class="form-control" id="inputRazaoSocial" style="max-width: 75%"
                                        placeholder="Entre com o nome da Empresa" required>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label for="cnpjEmpresa">CNPJ:</label>
                                    <input type="text" class="form-control cnpj" id="inputCnpj" style="max-width: 75%"
                                        required>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-6">
                                <div class="form-group">
                                    <label for="nomeFantasia">Nome Fantasia:</label>
                                    <input type="text" class="form-control" id="inputNomeFantasia"
                                        style="max-width: 75%" placeholder="Nome Fantasia" required>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-6">
                                <div class="form-group">
                                    <div class="form-group">
                                        <label for="inscricaoEstadual">Inscrição Estadual</label>
                                        <input type="text" class="form-control" id="inputInscricaoEstadual"
                                            style="max-width: 75%" placeholder="Inscrição Estadual" required>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-6">
                                <div class="form-group">
                                    <label for="inscricaoMunicipal">Inscrição Municipal</label>
                                    <input type="text" data-val="true" class="form-control" id="inputInscricaoMunicipal"
                                        style="max-width: 75%" placeholder="Inscrição Municipal">
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-6">
                                <div class="form-group">
                                    <label for="emailEmpresa">Email:</label>
                                    <input type="email" class="form-control" id="inputEmailEmpresa"
                                        style="max-width: 75%" placeholder="Email da empresa" required>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-6">
                                <div class="form-group">
                                    <label for="cepEmpresa">CEP:</label>
                                    <input type="text" class="form-control cep" id="inputCepEmpresa" style="max-width: 45%"
                                        placeholder="CEP da Empresa" required>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-6">
                                <div class="form-group">
                                    <label for="ruaEmpresa">Rua:</label>
                                    <input type="text" class="form-control" id="inputRuaEmpresa" style="max-width: 75%"
                                        placeholder="Rua da Empresa" required>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-6">
                                <div class="form-group">
                                    <label for="numeroEmpresa">Número:</label>
                                    <input type="text" class="form-control" id="inputNumeroEmpresa"
                                        style="max-width: 41%" placeholder="Número da empresa" required>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-6">
                                <div class="form-group">
                                    <label for="bairroEmpresa">Bairro:</label>
                                    <input type="text" class="form-control" id="inputBairroEmpresa"
                                        style="max-width: 75%" placeholder="Bairro da empresa" required>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-6">
                                <div class="form-group">
                                    <label for="cidadeEmpresa">Cidade:</label>
                                    <input type="text" class="form-control" id="inputCidadeEmpresa"
                                        style="max-width: 75%" placeholder="Cidade da empresa" required>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-6">
                                <div class="form-group">
                                    <label for="uf">Estado:</label>
                                    <input type="text" class="form-control" id="inputUfEmpresa" style="max-width: 50%"
                                        placeholder="Estado da empresa" required>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-6">
                                <div class="form-group">
                                    <label for="complementoEmpresa">Complemento:</label>
                                    <input type="text" class="form-control" id="inputComplementoEmpresa"
                                        style="max-width: 75%" placeholder="Complemento">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="card card-default">
                    <div class="card-header ">
                        <h3 class="card-title">Socio</h3>
